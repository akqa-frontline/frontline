    "@akqa-frontline/webpack-config": "file:../packages/webpack-config",
    "cross-env": "^6.0.3",
    "rimraf": "^3.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@babel/runtime-corejs3": "^7.7.2",
