    "@akqa-frontline/js-config-webpack-plugin": "file:../packages/js-config-webpack-plugin",
    "@akqa-frontline/scss-config-webpack-plugin": "file:../packages/scss-config-webpack-plugin",
    "@akqa-frontline/webpack-config": "file:../packages/webpack-config",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@types/react": "^16.9.14",
    "cross-env": "^6.0.3",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
