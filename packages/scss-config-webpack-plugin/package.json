{
  "name": "@akqa-frontline/scss-config-webpack-plugin",
  "version": "0.7.2-alpha.2",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose --runInBand --detectOpenHandles",
    "test-watch": "jest --verbose --watch --runInBand --detectOpenHandles",
    "test-coverage": "jest --verbose --coverage --runInBand --detectOpenHandles",
    "update-snapshots": "jest -u --runInBand"
  },
  "peerDependencies": {
    "node-sass": ">=4.0.0",
    "webpack": ">=4.0.0"
  },
  "dependencies": {
    "autoprefixer": "9.7.2",
    "css-loader": "3.2.1",
    "file-loader": "4.3.0",
    "iconfont-webpack-plugin": "4.2.1",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass-glob-importer": "5.3.2",
    "node-sass-json-importer": "4.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.1",
    "url-loader": "2.3.0",
    "webpack": "4.41.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/jsdom": "12.2.4",
    "@types/node": "12.12.11",
    "@types/webpack": "4.41.0",
    "eslint": "6.7.2",
    "jest": "24.9.0",
    "jsdom": "15.2.1",
    "node-sass": "^4.13.0",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "browser": false,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "<rootDir>/test/fixtures"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "fixtures",
      "dist"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!test/jsDomWindowContext.{js,ts}"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "engines": {
    "npm": ">=5",
    "node": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3d3a5638e09e5d6f428467189dd9ada6fad3110d"
}
