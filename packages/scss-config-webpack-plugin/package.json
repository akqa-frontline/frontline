{
  "name": "@akqa-frontline/scss-config-webpack-plugin",
  "version": "0.9.0-rc.1",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose --runInBand --detectOpenHandles",
    "test-watch": "jest --verbose --watch --runInBand --detectOpenHandles",
    "test-coverage": "jest --verbose --coverage --runInBand --detectOpenHandles",
    "update-snapshots": "jest -u --runInBand"
  },
  "peerDependencies": {
    "sass": ">=1.3.0",
    "webpack": ">=4.6.0"
  },
  "dependencies": {
    "autoprefixer": "10.1.0",
    "cosmiconfig": "7.0.0",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "iconfont-webpack-plugin": "5.0.0",
    "mini-css-extract-plugin": "1.3.2",
    "node-sass-glob-importer": "5.3.2",
    "node-sass-json-importer": "4.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss": "^8.2.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.1.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "resolve-url-loader": "3.1.2",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/jsdom": "12.2.4",
    "@types/node": "12.12.11",
    "@types/sass": "^1.16.0",
    "eslint": "6.7.2",
    "jest": "24.9.0",
    "jsdom": "15.2.1",
    "rimraf": "3.0.0",
    "sass": "^1.30.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "browser": false,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "<rootDir>/test/fixtures"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "fixtures",
      "dist"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!test/jsDomWindowContext.{js,ts}"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "engines": {
    "npm": ">=5",
    "node": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "660c006d106ac82acb557197568e08509adb981a"
}
