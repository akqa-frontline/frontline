{
	"name": "@akqa-frontline/webpack-config",
	"version": "0.9.0-rc.1",
	"license": "ISC",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "jest --verbose --runInBand --detectOpenHandles",
		"test-watch": "jest --verbose --watch --runInBand --detectOpenHandles",
		"test-coverage": "jest --verbose --coverage --runInBand --detectOpenHandles",
		"update-snapshots": "jest -u --runInBand"
	},
	"peerDependencies": {
		"@hot-loader/react-dom": "^16.12.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-hot-loader": "^4.12.18",
		"webpack": ">=4.6.0",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"@hot-loader/react-dom": "16.12.0",
		"core-js": "3.8.1",
		"dotenv": "^8.2.0",
		"dotenv-expand": "^5.1.0",
		"html-webpack-plugin": "4.5.0",
		"react": "16.12.0",
		"react-dev-utils": "^10.0.0",
		"react-dom": "16.12.0",
		"react-hot-loader": "4.12.18",
		"webpack": "4.44.2",
		"webpack-assets-manifest": "4.0.0",
		"webpack-bundle-analyzer": "4.2.0",
		"webpack-dev-server": "3.9.0",
		"webpack-fix-style-only-entries": "0.6.0"
	},
	"devDependencies": {
		"@types/jest": "24.0.23",
		"@types/node": "12.12.11",
		"@types/react": "16.9.13",
		"@typescript-eslint/eslint-plugin": "2.8.0",
		"@typescript-eslint/parser": "2.8.0",
		"eslint": "6.7.2",
		"jest": "24.9.0",
		"rimraf": "3.0.0",
		"ts-jest": "24.1.0",
		"typescript": "3.7.2"
	},
	"jest": {
		"browser": false,
		"preset": "ts-jest",
		"testEnvironment": "node",
		"watchPathIgnorePatterns": [
			"<rootDir>/test/fixtures"
		],
		"coveragePathIgnorePatterns": [
			"node_modules",
			"fixtures",
			"dist"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!test/jsDomWindowContext.{js,ts}"
		],
		"coverageReporters": [
			"text"
		]
	},
	"engines": {
		"npm": ">=5",
		"node": ">=6"
	},
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "660c006d106ac82acb557197568e08509adb981a"
}
