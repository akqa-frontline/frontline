{
	"name": "@akqa-frontline/webpack-config",
	"version": "0.2.0",
	"license": "MIT",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "jest --verbose --runInBand --detectOpenHandles",
		"test-watch": "jest --verbose --watch --runInBand --detectOpenHandles",
		"test-coverage": "jest --verbose --coverage --runInBand --detectOpenHandles",
		"update-snapshots": "jest -u --runInBand"
	},
	"peerDependencies": {
		"@hot-loader/react-dom": "^16.11.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-hot-loader": "^4.12.18",
		"webpack": ">=4.0.0",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"@babel/core": "7.7.4",
		"@babel/plugin-proposal-class-properties": "7.7.0",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.6.2",
		"@babel/preset-env": "7.7.4",
		"@babel/preset-react": "7.7.4",
		"@babel/runtime": "7.7.4",
		"@babel/runtime-corejs3": "7.7.4",
		"@hot-loader/react-dom": "16.11.0",
		"browserslist": "4.7.3",
		"core-js": "3.4.5",
		"html-webpack-plugin": "3.2.0",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-hot-loader": "4.12.18",
		"webpack": "4.41.2",
		"webpack-bundle-analyzer": "3.6.0",
		"webpack-dev-server": "3.9.0",
		"webpack-manifest-plugin": "^2.2.0",
		"webpack-module-nomodule-plugin": "^0.2.1"
	},
	"devDependencies": {
		"@types/browserslist": "4.4.0",
		"@types/html-webpack-plugin": "^3.2.1",
		"@types/jest": "24.0.23",
		"@types/node": "12.12.11",
		"@types/react": "16.9.13",
		"@types/webpack": "4.41.0",
		"@types/webpack-bundle-analyzer": "2.13.3",
		"@types/webpack-dev-server": "3.4.0",
		"@types/webpack-manifest-plugin": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "2.8.0",
		"@typescript-eslint/parser": "2.8.0",
		"eslint": "6.7.2",
		"jest": "24.9.0",
		"rimraf": "3.0.0",
		"ts-jest": "24.1.0",
		"typescript": "3.7.2"
	},
	"jest": {
		"browser": false,
		"preset": "ts-jest",
		"testEnvironment": "node",
		"watchPathIgnorePatterns": [
			"<rootDir>/test/fixtures"
		],
		"coveragePathIgnorePatterns": [
			"node_modules",
			"fixtures",
			"dist"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!test/jsDomWindowContext.{js,ts}"
		],
		"coverageReporters": [
			"text"
		]
	},
	"engines": {
		"npm": ">=5",
		"node": ">=6"
	},
	"publishConfig": {
		"access": "public"
	}
}
